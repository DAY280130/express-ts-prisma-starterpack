{
  "name": "express-ts-prisma-starterpack",
  "version": "0.0.1",
  "description": "dimas's express, ts, and prisma project starterpack",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npx tsx ./src/index.ts",
    "dev:watch": "npx tsx watch ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "mkdir dist && cp package.prod.json ./dist/package.json && ttsc -p tsconfig.prod.json",
    "build:clean": "npm run clean && npm run build",
    "clean": "npx rimraf ./dist"
  },
  "author": "Dimas Adnan",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.3",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.5",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "https": "^1.0.0",
    "prisma": "^4.11.0"
  }
}
